{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "plugins": ["jest"],
  "rules": {
    "no-console": 0,
    "no-unused-vars": "off",
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "jest/prefer-to-have-length": "warn",
    "jest/valid-expect": "error"
  }  
}